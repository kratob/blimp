%form(ng-submit='dashboard.goTo(channelName)' layout='row' layout-align='start center')
  %md-input-container.inline-input(md-no-float flex-gt-sm=40)
    %input(type='text' ng-model='channelName' ng-change='channel.filter(query)' placeholder='Channel name')
  %md-button.md-primary.md-raised
    Go!
